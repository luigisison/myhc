	                	<div class="oe_title oe_left">					
							<h1>					
								<field name="name" default_focus="1" placeholder="Name" />				
							</h1>					
						</div>					
						<group>					
	                    	<field name="code"/>				
	                    	<field name="state_id"/>				
	                    	<field name="country_id"/>				
	                    	<separator string="Postal Codes" colspan="4"/>				
	                    	<field name="postal_code_ids" colspan="4" nolabel="1"/>				
						</group>					
